{
	"expo": {
		"name": "postertrack",
		"slug": "postertrack",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./src/assets/images/icon.png",
		"scheme": "postertrack",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/images/poster-track-logo.png",
				"backgroundColor": "#140100"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./src/assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./src/assets/images/poster-track-logo.png",
					"imageWidth": 100,
					"resizeMode": "contain",
					"backgroundColor": "#140100"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
